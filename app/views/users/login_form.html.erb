<div class="signup-container">
  <%= form_with url:"/login" do |f| %>
    <div class="signup-content">
      <%= f.label :メールアドレス,class:"signup-label" %>
      <br>
      <%= f.text_field :email,class: "signup-area"%>
    </div>
    <div class="signup-content">
      <%= f.label :パスワード,class:"signup-label" %>
      <br>
      <%= f.password_field :password,class: "signup-area" %>
    </div>
    <div class="button-container">
      <%= f.submit :ログイン,class: "button" %>
    </div>
  <% end %>
</div>